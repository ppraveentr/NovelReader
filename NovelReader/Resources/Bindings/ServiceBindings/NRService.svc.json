{
    "NovelBase": {
        "timeout": "60",
        "x-api-key": "bdb6d57f4df64420aa0b220cd924f85e"
    },
    "fetchRecentUpdatesList": {
        "super": "NovelBase",
        "type": "GET",
        "path": "novel/recentupdates",
        "responseType": {
            "bindAs": "NRService_fetchRecentUpdatesListResponse"
        }
    },
    "fetchNovelList": {
        "super": "NovelBase",
        "type": "GET",
        "path": "novel/top-list",
        "requestQuery": {
            "type": "type",
            "category": "category",
            "state": "state",
            "page": "page"
        },
        "responseType": {
            "arrayOf": "NRNovel"
        }
    },
    "fetchNovelChapters": {
        "super": "NovelBase",
        "type": "GET",
        "path": "novel/chapters-list",
        "responseType": {
            "bindAs": "NRNovel"
        }
    },
    "fetchChapter": {
        "super": "NovelBase",
        "type": "GET",
        "path": "novel/chapter",
        "responseType": {
            "bindAs": "NRNovelChapter"
        }
    },
    "searchNovel": {
        "super": "NovelBase",
        "type": "POST",
        "path": "novel/search",
        "responseType": {
            "bindAs": "NRServiceResponse_searchNovel"
        }
    }
}
