{
	"type": "Themes",
	"color": {
		"default": "#000000",
		"AA": "#16c5ce",
		"clear": "clear",
		"black": "#000000",
		"appRed": "#DD6060",
		"navBarRed": "#DF6E6E",
		"appWhite": "#F7F3F0",
        "halfBlack": "#9A9A9A99",
		"blue": "#1059ce",
		"green": "#10ce1a",
		"yellow": "#cecb10",
		"white": "#FFFFFF",
		"gray": "#A9A9A9"
	},
	"font": {
		"default": {
			"name": "system",
			"size": "13.0"
		},
        "bold": {
            "name": "boldSystem",
            "size": "17.0"
        },
		"system14": {
			"_super": "default",
			"size": "14.0"
		},
		"system17": {
			"_super": "default",
			"size": "17.0"
		},
        "bold17": {
            "name": "boldSystem",
            "size": "17.0"
        }
	},
	"appearance": {
		"UIStatusBar": {
			"tintColor": "white"
		},
		"UINavigationBar:UINavigationController": {
			"barTintColor": "white",
			"tintColor": "white",
			"isTranslucent": true,
			"backgroundImage": {
				"default": "@Pixel",
				"landScape": "@Pixel"
			},
			"shadowImage": "@empty",
			"titleText": {
				"foregroundColor": "white"
			}
		},
		"UIToolbar:UINavigationController": {
			"barTintColor": "white",
			"isTranslucent": true
		},
		"UISegmentedControl": {
			"tintColor": "appRed"
		},
		"UISegmentedControl:UICollectionReusableView": {
			"tintColor": "white"
		}
	},
	"components": {
		"FTView": {
			"default": {
				"backgroundColor": "appWhite"
			},
            "clearView": {
                "backgroundColor": "clear"
            },
            "halfView": {
                "backgroundColor": "halfBlack"
            }
		},
        "NRSectionHeaderView": {
            "default": {
                "backgroundColor": "appWhite"
            }
        },
        "UICollectionView": {
            "default": {
                "backgroundColor": "clear"
            },
            "halfView": {
                "backgroundColor": "appWhite",
                "backgroundViewTheme": "default"
            }
        },
		"NRSegmentCollectionHeaderView": {
			"default": {
				"backgroundColor": "navBarRed"
			}
		},
        "NRSearchBarHeaderView": {
            "default": {
                "backgroundColor": "navBarRed"
            }
        },
        "FTSearchBar": {
            "default": {
                "textcolor": "white",
                "tintColor": "white",
                "barTintColor": "navBarRed"
            }
        },
		"FTButton": {
			"default": {
				"textfont": "system14",
				"textcolor": "black",
				"backgroundColor": "clear"
			},
			"button14R": {
				"_super": "default",
				"textcolor": "appRed"
			},
			"button14R:highlighted": {
				"_super": "default",
				"textcolor": "green"
			},
			"button14R:disabled": {
				"_super": "default",
				"textcolor": "yellow"
			},
            "buttonR17bW": {
                "textfont": "bold17",
                "textcolor": "white",
                "backgroundColor": "appRed"
            },
			"googleButton": {
				"_super": "default",
				"image": "@google",
				"backgroundColor": "white",
				"layer": {
					"cornerRadius": 22
				}
			}
		},
		"FTLabel": {
			"default": {
				"textfont": "system14",
				"textcolor": "black",
				"backgroundColor": "clear",
				"isLinkUnderlineEnabled": true,
				"isLinkDetectionEnabled": true
			},
			"system13Gray": {
				"_super": "default",
				"textcolor": "gray"
			},
			"system17R": {
				"_super": "default",
				"textfont": "system17",
				"textcolor": "appRed"
			},
			"selectionCell": {
				"_super": "default",
                "textfont": "system17",
				"textcolor": "appRed"
			},
            "selectionCell:disabled": {
                "_super": "default",
                "textcolor": "gray"
            }
		}
	}
}
